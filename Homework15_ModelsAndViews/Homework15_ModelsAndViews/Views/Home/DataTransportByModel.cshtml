@model Fish[]
@{
}
<ul>
    @for(int i = 0; i < Model.Length; i++ )
    {
        @if(i == 0)
        {
            <li>
                @("Name: " + ViewData["Karp"] + " Weight: " + Model[i].Weight + " Speed: " + Model[i].Speed)
            </li>
            continue;
        }
        @if(i == Model.Length - 1)
        {
            <li>
                @("Name: " + Model[i].Name + " Weight: " + Model[i].Weight + " Speed: " + ViewBag.Ten)
            </li>
            continue;
        }
        <li>
            @("Name: " + Model[i].Name + " Weight: " + Model[i].Weight + " Speed: " + Model[i].Speed)
        </li>
    }

</ul>
