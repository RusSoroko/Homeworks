@{
    var fish = ViewBag.FishBag as Fish[];
}

<ul>
    @foreach (var fish1 in fish)
    {
        <li>@("Name: " + fish1.Name + " Weight: " + fish1.Weight + " Speed: " + fish1.Speed)</li>
    }
</ul>
